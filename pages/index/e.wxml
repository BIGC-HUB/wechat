<view class="location">

</view>

<view class="card">
    <scroll-view scroll-x
    scroll-into-view="{{toView}}"
    bindtouchstart="start"
    bindtouchend="end"
    bindscrolltoupper="left"
    bindscrolltolower="right"
    scroll-with-animation >
        <block wx:for="{{order}}" wx:key="key">
          <view id="{{item}}" style="background: {{item}}"> {{index}}: {{item}}</view>
        </block>
    </scroll-view>
</view>

<view class="bottom">

</view>


