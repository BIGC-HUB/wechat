<view class="tan">
    <view class="location">

    </view>

    <view class="card">
        <view class="we-container {{directionClass}}">
          <view class="we-wrapper"
            bindtouchstart="touchstart"
            bindtouchmove="touchmove"
            bindtouchend="touchend"
            animation="{{animationData}}">
            <block wx:for="{{order}}" wx:key="key">
              <view class="we-slide">
                  {{index}} : {{item}}
              </view>
            </block>
          </view>
        </view>
    </view>

    <view class="bottom">

    </view>
</view>
